  "dependencies": {
    "@remotion/cli": "4.0.230",
    "@remotion/transitions": "4.0.230",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remotion": "4.0.230"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "typescript": "^5.4.5"
  }
}
