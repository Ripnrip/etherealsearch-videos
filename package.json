{
  "name": "etherealsearch-videos",
  "version": "1.0.0",
  "description": "EtherealSearch Demo Videos with Remotion",
  "scripts": {
    "start": "remotion studio",
    "build": "remotion render Main out/video.mp4",
    "render:hero": "remotion render HeroDemo out/hero-demo.mp4",
